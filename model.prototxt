name: "TCYB2018"
layer {
  name: "imagenet_data"
  type: "ImageData"
  top: "imagenet_data"
  top: "imagenet_label"
  transform_param {
    mirror: false
    crop_size: 224
  }
  image_data_param {
    source: "dataset/ImageNet/train_sample.txt"   #ImageNet List
    batch_size: 64
    shuffle: true
    root_folder: "dataset/ImageNet/train_sample/" #ImageNet folder
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_img_data"
  type: "ImageData"
  top: "xmedia_img_data"
  top: "xmedia_img_label"
  transform_param {
    mirror: false
    crop_size: 224
  }
  image_data_param {
    source: "dataset/xmedia/train_img.txt" #XMedia Training Image List
    batch_size: 64
    shuffle: false
    root_folder: "dataset/xmedia/image/" #XMedia image folder
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_img_data"
  type: "ImageData"
  top: "data"
  top: "xmedia_img_label"
  transform_param {
    mirror: false
    crop_size: 224
  }
  image_data_param {
    source: "dataset/xmedia/test_img.txt" #XMedia Testing Image List
    batch_size: 10
    shuffle: false
    root_folder: "dataset/xmedia/image/" #XMedia image folder
  }
  include: { phase: TEST }
}

layer {
  name: "xmedia_txt_data"
  type: "Data"
  top: "xmedia_txt_data"
  top: "xmedia_txt_label"
  data_param {
    source: "dataset/xmedia/train_txt_lmdb/" #XMedia Training Text lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_txt_data"
  type: "Data"
  top: "xmedia_txt_data"
  top: "xmedia_txt_label"
  data_param {
    source: "dataset/xmedia/test_txt_lmdb/" #XMedia Testing Text lmdb
    batch_size: 10
    backend: LMDB
  }
  include: { phase: TEST }
}

layer {
  name: "xmedia_aud_data"
  type: "Data"
  top: "xmedia_aud_data"
  top: "xmedia_aud_label"
  data_param {
    source: "dataset/xmedia/train_aud_znorm_lmdb/" #XMedia Training Audio lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_aud_data"
  type: "Data"
  top: "xmedia_aud_data"
  top: "xmedia_aud_label"
  data_param {
    source: "dataset/xmedia/test_aud_znorm_lmdb/" #XMedia Testing Audio lmdb
    batch_size: 2
    backend: LMDB
  }
  include: { phase: TEST }
}

layer {
  name: "xmedia_vid_data"
  type: "Data"
  top: "xmedia_vid_data"
  top: "xmedia_vid_label"
  data_param {
    source: "dataset/xmedia/train_vid_lmdb/" #XMedia Training Video lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_vid_data"
  type: "Data"
  top: "xmedia_vid_data"
  top: "xmedia_vid_label"
  data_param {
    source: "dataset/xmedia/test_vid_lmdb/" #XMedia Testing Video lmdb
    batch_size: 1
    backend: LMDB
  }
  include: { phase: TEST }
}

layer {
  name: "xmedia_3d_data"
  type: "Data"
  top: "xmedia_3d_data"
  top: "xmedia_3d_label"
  data_param {
    source: "dataset/xmedia/train_3d_lmdb/" #XMedia Training 3D Model lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_3d_data"
  type: "Data"
  top: "xmedia_3d_data"
  top: "xmedia_3d_label"
  data_param {
    source: "dataset/xmedia/test_3d_lmdb/" #XMedia Testing 3D Model lmdb
    batch_size: 1
    backend: LMDB
  }
  include: { phase: TEST }
}

layer {
  name: "similarity"
  type: "DummyData"
  top: "similarity"
  dummy_data_param {
    data_filler {
      type: "constant"
      value: 1
    }
    num: 64
    channels: 1
    height: 1
    width: 1
  }
  include: { phase: TRAIN }
}

layer {
  name: "xmedia_img_modal_label"
  type: "Data"
  top: "xmedia_img_modal_label"
  top: "xmedia_img_modal_label_none"
  data_param {
    source: "dataset/xmedia/one-hot/train_img_onehot_lmdb/" #XMedia Training Image Modality Labels lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_img_modal_label_none_silence"
  type: "Silence"
  bottom: "xmedia_img_modal_label_none"
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_txt_modal_label"
  type: "Data"
  top: "xmedia_txt_modal_label"
  top: "xmedia_txt_modal_label_none"
  data_param {
    source: "dataset/xmedia/one-hot/train_txt_onehot_lmdb/" #XMedia Training Text Modality Labels lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_txt_modal_label_none_silence"
  type: "Silence"
  bottom: "xmedia_txt_modal_label_none"
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_aud_modal_label"
  type: "Data"
  top: "xmedia_aud_modal_label"
  top: "xmedia_aud_modal_label_none"
  data_param {
    source: "dataset/xmedia/one-hot/train_aud_onehot_lmdb/" #XMedia Training Audio Modality Labels lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_aud_modal_label_none_silence"
  type: "Silence"
  bottom: "xmedia_aud_modal_label_none"
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_vid_modal_label"
  type: "Data"
  top: "xmedia_vid_modal_label"
  top: "xmedia_vid_modal_label_none"
  data_param {
    source: "dataset/xmedia/one-hot/train_vid_onehot_lmdb/" #XMedia Training Video Modality Labels lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_vid_modal_label_none_silence"
  type: "Silence"
  bottom: "xmedia_vid_modal_label_none"
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_3d_modal_label"
  type: "Data"
  top: "xmedia_3d_modal_label"
  top: "xmedia_3d_modal_label_none"
  data_param {
    source: "dataset/xmedia/one-hot/train_3d_onehot_lmdb/" #XMedia Training 3D Model Modality Labels lmdb
    batch_size: 64
    backend: LMDB
  }
  include: { phase: TRAIN }
}
layer {
  name: "xmedia_3d_modal_label_none_silence"
  type: "Silence"
  bottom: "xmedia_3d_modal_label_none"
  include: { phase: TRAIN }
}
layer {
  name: "concat_modal_label"
  type: "Concat"
  bottom: "xmedia_img_modal_label"
  bottom: "xmedia_txt_modal_label"
  bottom: "xmedia_aud_modal_label"
  bottom: "xmedia_vid_modal_label"
  bottom: "xmedia_3d_modal_label"
  top: "xmedia_modal_label"
  concat_param {
    concat_dim: 0
  }
  include: { phase: TRAIN }
}

# ---------------------------------------------------------- source and target data concatenation

layer {
  name: "concat_data"
  type: "Concat"
  bottom: "imagenet_data"
  bottom: "xmedia_img_data"
  top: "data"
  concat_param {
    concat_dim: 0
  }
  include: { phase: TRAIN }
}

# ---------------------------------------------------------- convolution

layer {
  name: "data/bn"
  type: "BatchNorm"
  bottom: "data"
  top: "data/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "data/scale"
  type: "Scale"
  bottom: "data/bn"
  top: "data/scale"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  scale_param {
    bias_term: true
  }
}
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data/scale"
  top: "conv1"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 96
    kernel_size: 11
    stride: 4
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv1/bn"
  type: "BatchNorm"
  bottom: "conv1"
  top: "conv1/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "conv1/bn"
  top: "norm1"
}
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "norm1"
  top: "pool1"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "pool1"
  top: "conv2"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 2
    kernel_size: 5
    group: 2
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv2/bn"
  type: "BatchNorm"
  bottom: "conv2"
  top: "conv2/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu2"
  type: "ReLU"
  bottom: "conv2/bn"
  top: "norm2"
}
layer {
  name: "pool2"
  type: "Pooling"
  bottom: "norm2"
  top: "pool2"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "pool2"
  top: "conv3"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 384
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv3/bn"
  type: "BatchNorm"
  bottom: "conv3"
  top: "conv3/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu3"
  type: "ReLU"
  bottom: "conv3/bn"
  top: "conv3/bn"
}
layer {
  name: "conv4"
  type: "Convolution"
  bottom: "conv3/bn"
  top: "conv4"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 384
    pad: 1
    kernel_size: 3
    group: 2
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv4/bn"
  type: "BatchNorm"
  bottom: "conv4"
  top: "conv4/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu4"
  type: "ReLU"
  bottom: "conv4/bn"
  top: "conv4/bn"
}
layer {
  name: "conv5"
  type: "Convolution"
  bottom: "conv4/bn"
  top: "conv5"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
    group: 2
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv5/bn"
  type: "BatchNorm"
  bottom: "conv5"
  top: "conv5/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu5"
  type: "ReLU"
  bottom: "conv5/bn"
  top: "conv5/bn"
}
layer {
  name: "pool5"
  type: "Pooling"
  bottom: "conv5/bn"
  top: "pool5"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}

# ---------------------------------------------------------- alias pool5 to xmedia_img_pool5 in test

layer {
  name: "pool5_alias"
  type: "Split"
  bottom: "pool5"
  top: "xmedia_img_pool5"
  include: { phase: TEST }
}

# ---------------------------------------------------------- split source and target in train

layer {
  name: "slice_pool5"
  type: "Slice"
  bottom: "pool5"
  top: "imagenet_pool5"
  top: "xmedia_img_pool5"
  slice_param {
    slice_dim: 0
  }
  include: { phase: TRAIN }
}

# ---------------------------------------------------------- fc6 of source

layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "imagenet_pool5"
  top: "fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
  include: { phase: TRAIN }
}
layer {
  name: "fc6/bn"
  type: "BatchNorm"
  bottom: "fc6"
  top: "fc6/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
  include: { phase: TRAIN }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "fc6/bn"
  top: "fc6/bn"
  include: { phase: TRAIN }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "fc6/bn"
  top: "fc7"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
  include: { phase: TRAIN }
}
layer {
  name: "fc7/bn"
  type: "BatchNorm"
  bottom: "fc7"
  top: "fc7/bn"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  batch_norm_param {
    moving_average_fraction: 0.9
  }
  include: { phase: TRAIN }
}
layer {
  name: "relu7"
  type: "ReLU"
  bottom: "fc7/bn"
  top: "fc7/bn"
  include: { phase: TRAIN }
}  
layer {
  name: "fc8"
  type: "InnerProduct"
  bottom: "fc7/bn"
  top: "fc8"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1000
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
  include: { phase: TRAIN }
}

# ---------------------------------------------------------- fc6 of target

layer {
  name: "xmedia_img_fc6"
  type: "InnerProduct"
  bottom: "xmedia_img_pool5"
  top: "xmedia_img_fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "xmedia_img_fc6/bn"
